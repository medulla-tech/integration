[main]
{% if URBACKUP_DISABLED %}
disable = 1
{% else %}
disable = 0
{% endif %}

[database]
dbhost = {{ DBHOST }}
dbport = {{ DBPORT }}
dbuser = {{ DBUSER }}
dbpasswd = {{ DBPASSWORD }}

[urbackup]
url = http://{{ URBACKUP_API_SERVER }}/urbackup/x
password = {{ URBACKUP_ADMINPASSWD }}
passwordapi = {{ URBACKUP_API_PASSWORD }}
